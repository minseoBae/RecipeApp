<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.home.ProfileFragment"
        tools:layout_editor_absoluteX="-13dp"
        tools:layout_editor_absoluteY="6dp">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:background="@drawable/profile_top"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/profileImage"
                android:layout_width="115dp"
                android:layout_height="115dp"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="30dp"
                app:srcCompat="@drawable/user_circle" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="124dp"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/userName1"
                    android:layout_width="185dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="53dp"
                    android:fontFamily="@font/semi"
                    android:text="TextView"
                    android:textColor="@color/white"
                    android:textSize="25sp" />

                <TextView
                    android:id="@+id/localdate"
                    android:layout_width="148dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:fontFamily="@font/semi"
                    android:text="TextView"
                    android:textColor="@color/white"
                    android:textSize="10sp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="330dp"
            android:layout_height="500dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
            app:layout_constraintVertical_bias="0.153">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="12dp"
                android:text="NAME"
                android:textColor="@color/slategrey"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/userName"
                android:layout_width="300dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                android:textColor="@color/grey"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/top_line" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="12dp"
                android:text="EMAIL"
                android:textColor="@color/slategrey"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/email"
                android:layout_width="300dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                android:textColor="@color/grey"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/top_line" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="12dp"
                android:text="Address"
                android:textColor="@color/slategrey"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/address"
                android:layout_width="300dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                android:textColor="@color/grey"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/top_line" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="12dp"
                android:text="Age"
                android:textColor="@color/slategrey"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/age"
                android:layout_width="300dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                android:textColor="@color/grey"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/top_line" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="80dp"
                android:layout_height="20dp"
                android:layout_marginLeft="13dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="12dp"
                android:text="Phone"
                android:textColor="@color/slategrey"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/phone"
                android:layout_width="300dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:ems="10"
                android:inputType="text"
                android:text="Name"
                android:textColor="@color/grey"
                android:textSize="18sp" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/top_line" />

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>